    "browserslist-useragent": "^3.0.0",
    "del-cli": "^2.0.0",
    "rollup-plugin-add-shebang": "^0.3.0",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.24.0",
    "user-agents": "^1.0.213"
  },
  "files": [
    "lib"
  ]
