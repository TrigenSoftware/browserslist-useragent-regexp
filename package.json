{
  "name": "browserslist-useragent-regexp",
  "version": "0.0.0",
  "description": "A utility to compile a browselist query to regular expression.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/browserslist-useragent-regexp"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/browserslist-useragent-regexp/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit --pretty --skipLibCheck",
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "checkSize": "size-limit",
    "test": "npm run typecheck && npm run lint && npm run jest && npm run build && npm run checkSize",
    "build:docs": "typedoc ./src --out ./docs --ignoreCompilerErrors --excludeExternals --mode modules; touch docs/.nojekyll",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "cleanPublish": "npm run test && clean-publish"
  },
  "keywords": [
    "browserslist",
    "regexp"
  ],
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@types/node": "^11.11.3",
    "browserslist": "^4.3.6",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@types/jest": "^24.0.11",
    "babel-jest": "^24.3.0",
    "babel-preset-trigen": "^2.0.3",
    "browserslist-config-trigen": "^1.1.0",
    "clean-publish": "^1.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.12.0",
    "eslint-config-trigen": "^3.2.1",
    "husky": "^1.3.1",
    "jest": "^24.3.0",
    "lint-staged": "^8.1.0",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "size-limit": "^0.21.1",
    "tslint": "^5.12.0",
    "tslint-config-trigen": "^3.0.7",
    "typedoc": "^0.14.0",
    "typescript": "^3.2.2",
    "useragent-generator": "^1.1.1-amkt-22079-finish.0"
  },
  "files": [
    "lib"
  ]
}
